308 ? ((1.055 * Math.pow(g, 1.0 / 2.4)) - 0.055)\n\t\t: g = (g * 12.92);\n\n\tb = b > 0.0031308 ? ((1.055 * Math.pow(b, 1.0 / 2.4)) - 0.055)\n\t\t: b = (b * 12.92);\n\n\tr = Math.min(Math.max(0, r), 1);\n\tg = Math.min(Math.max(0, g), 1);\n\tb = Math.min(Math.max(0, b), 1);\n\n\treturn [r * 255, g * 255, b * 255];\n}\n\n\n\n/**\n * RGB to XYZ\n *\n * @param {Array} rgb RGB channels\n *\n * @return {Array} XYZ channels\n */\nrgb.xyz = function(rgb, white) {\n\tvar r = rgb[0] / 255,\n\t\t\tg = rgb[1] / 255,\n\t\t\tb = rgb[2] / 255;\n\n\t// assume sRGB\n\tr = r > 0.04045 ? Math.pow(((r + 0.055) / 1.055), 2.4) : (r / 12.92);\n\tg = g > 0.04045 ? Math.pow(((g + 0.055) / 1.055), 2.4) : (g / 12.92);\n\tb = b > 0.04045 ? Math.pow(((b + 0.055) / 1.055), 2.4) : (b / 12.92);\n\n\tvar x = (r * 0.41239079926595) + (g * 0.35758433938387) + (b * 0.18048078840183);\n\tvar y = (r * 0.21263900587151) + (g * 0.71516867876775) + (b * 0.072192315360733);\n\tvar z = (r * 0.019330818715591) + (g * 0.11919477979462) + (b * 0.95053215224966);\n\n\twhite = white || xyz.whitepoint[2].E;\n\n\treturn [x * white[0], y * white[1], z * white[2]];\n};\n\n\n\nexport default xyz;\n"],"mappings":"AAAA;AACA;AACA;AACA;AACA;AACA,OAAOA,GAAP,MAAgB,UAAhB;AAEA,IAAIC,GAAG,GAAG;EACTC,IAAI,EAAE,KADG;EAETC,GAAG,EAAE,CAAC,CAAD,EAAG,CAAH,EAAK,CAAL,CAFI;EAGTC,OAAO,EAAE,CAAC,GAAD,EAAK,GAAL,EAAS,GAAT,CAHA;EAITC,KAAK,EAAE,CAAC,KAAD,EAAQ,QAAR,EAAkB,SAAlB;AAJE,CAAV;AAQA;AACA;AACA;AACA;AACA;;AACAJ,GAAG,CAACK,UAAJ,GAAiB;EAChB;EACA,GAAG;IACF;IACAC,CAAC,EAAC,CAAC,MAAD,EAAS,GAAT,EAAc,MAAd,CAFA;IAGF;IACAC,CAAC,EAAE,CAAC,MAAD,EAAS,GAAT,EAAc,OAAd,CAJD;IAKFC,GAAG,EAAE,CAAC,MAAD,EAAS,GAAT,EAAc,MAAd,CALH;IAMFC,GAAG,EAAE,CAAC,MAAD,EAAS,GAAT,EAAc,MAAd,CANH;IAOF;IACAC,GAAG,EAAE,CAAC,eAAD,EAAkB,GAAlB,EAAuB,iBAAvB,CARH;IASFC,GAAG,EAAE,CAAC,MAAD,EAAS,GAAT,EAAc,OAAd,CATH;IAUF;IACA;IACAC,EAAE,EAAE,CAAC,MAAD,EAAS,GAAT,EAAc,MAAd,CAZF;IAaF;IACA;IACA;IACA;IACAC,EAAE,EAAE,CAAC,MAAD,EAAS,GAAT,EAAc,OAAd,CAjBF;IAkBF;IACA;IACA;IACAC,GAAG,EAAE,CAAC